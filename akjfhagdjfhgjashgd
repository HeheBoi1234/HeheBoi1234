viethub = {}
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
function viethub:Windown(gamename)
    local VietHUb = Instance.new("ScreenGui")
    local MainFrame = Instance.new("Frame")
    local Viet = Instance.new("TextLabel")
    local hub = Instance.new("TextLabel")
    local game = Instance.new("TextLabel")
    local Channel = Instance.new("ScrollingFrame")
    local UIGridLayout = Instance.new("UIGridLayout")
    local ContainerHolder = Instance.new("Frame")

    VietHUb.Name = "VietHUb"
    VietHUb.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    VietHUb.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    MainFrame.Name = "MainFrame"
    MainFrame.Parent = VietHUb
    MainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    MainFrame.Position = UDim2.new(0.308313817, 0, 0.106493324, 0)
    MainFrame.Size = UDim2.new(0, 390, 0, 350)
    MainFrame.Draggable = true

    Viet.Name = "Viet"
    Viet.Parent = MainFrame
    Viet.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Viet.BackgroundTransparency = 1.000
    Viet.Position = UDim2.new(0.0182370823, 0, 0, 0)
    Viet.Size = UDim2.new(0, 323, 0, 20)
    Viet.Font = Enum.Font.Gotham
    Viet.Text = "Viet"
    Viet.TextColor3 = Color3.fromRGB(255, 255, 255)
    Viet.TextSize = 14.000
    Viet.TextXAlignment = Enum.TextXAlignment.Left

    hub.Name = "hub"
    hub.Parent = Viet
    hub.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    hub.BackgroundTransparency = 1.000
    hub.Position = UDim2.new(0.0928792581, 0, 0, 0)
    hub.Size = UDim2.new(0, 25, 0, 20)
    hub.Font = Enum.Font.Gotham
    hub.Text = "Hub"
    hub.TextColor3 = Color3.fromRGB(203, 62, 49)
    hub.TextSize = 14.000
    hub.TextXAlignment = Enum.TextXAlignment.Left

    game.Name = "game"
    game.Parent = Viet
    game.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    game.BackgroundTransparency = 1.000
    game.Position = UDim2.new(0.188854486, 0, 0, 0)
    game.Size = UDim2.new(0, 262, 0, 20)
    game.Font = Enum.Font.Gotham
    game.Text = "| "..gamename
    game.TextColor3 = Color3.fromRGB(255, 255, 255)
    game.TextSize = 14.000
    game.TextXAlignment = Enum.TextXAlignment.Left

    Channel.Name = "Channel"
    Channel.Parent = MainFrame
    Channel.Active = true
    Channel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Channel.BorderColor3 = Color3.fromRGB(27, 42, 53)
    Channel.Position = UDim2.new(0.018236991, 0, 0.0615384579, 0)
    Channel.Size = UDim2.new(0, 369, 0, 18)
    Channel.CanvasSize = UDim2.new(2, 0, 0, 0)
    Channel.ScrollBarThickness = 1

    UIGridLayout.Parent = Channel
    UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIGridLayout.VerticalAlignment = Enum.VerticalAlignment.Center
    UIGridLayout.CellSize = UDim2.new(0, 60, 0, 19)

    ContainerHolder.Name = "ContainerHolder"
    ContainerHolder.Parent = MainFrame
    ContainerHolder.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ContainerHolder.BorderColor3 = Color3.fromRGB(50, 50, 50)
    ContainerHolder.Position = UDim2.new(0, 0, 0.154285714, 0)
    ContainerHolder.Size = UDim2.new(0, 390, 0, 296)
    win = {}
    function win:Channel(name)
        local ScrollingFrame = Instance.new("ScrollingFrame")
        local UIListLayout = Instance.new("UIListLayout")

        ScrollingFrame.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder
        ScrollingFrame.Active = true
        ScrollingFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        ScrollingFrame.BorderColor3 = Color3.fromRGB(50, 50, 50)
        ScrollingFrame.Size = UDim2.new(0, 390, 0, 299)
        ScrollingFrame.ScrollBarThickness = 2

        UIListLayout.Parent = ScrollingFrame
        UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Padding = UDim.new(0, 10)

        local TextButton = Instance.new("TextButton")

        TextButton.Parent = game.StarterGui.VietHUb.MainFrame.Channel
        TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.BackgroundTransparency = 1.000
        TextButton.Position = UDim2.new(0, 0, -2.27777767, 0)
        TextButton.Size = UDim2.new(0, 67, 0, 100)
        TextButton.Font = Enum.Font.SourceSans
        TextButton.TextColor3 = Color3.fromRGB(180, 180, 180)
        TextButton.TextSize = 14.000
        TextButton.Text = name
        
        TextButton.MouseButton1Click:Connect(function()
            for _, co in pairs(ContainerHolder:GetChildren()) do
                if co:IsA("ScrollingFrame") then
                    co.Visible = false
                end
            end
            TextButton.TextColor3 = Color3.fromRGB(255, 159, 48)
            ScrollingFrame.Visible = true
        end)
        content = {}
        function content:Button(name,call)
            local Button = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local ImageLabel = Instance.new("ImageLabel")

            Button.Name = "Button"
            Button.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder.ScrollingFrame
            Button.BackgroundColor3 = Color3.fromRGB(144, 89, 34)
            Button.Position = UDim2.new(-0.189743593, 0, 0, 0)
            Button.Size = UDim2.new(0, 350, 0, 25)
            Button.Font = Enum.Font.Gotham
            Button.Text = name
            Button.TextColor3 = Color3.fromRGB(255, 255, 255)
            Button.TextSize = 14.000

            UICorner.CornerRadius = UDim.new(0.0399999991, 0)
            UICorner.Parent = Button

            ImageLabel.Parent = Button
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.BorderColor3 = Color3.fromRGB(27, 42, 53)
            ImageLabel.Position = UDim2.new(0.908571482, 0, 0, 0)
            ImageLabel.Size = UDim2.new(0, 25, 0, 25)
            ImageLabel.Image = "rbxassetid://7839505809"

            Button.MouseButton1Click:Connect(function()
                pcall(function()
                    call()
                end)
            end)
        end

        function content:Toggle(name,value,call)
            local Toggle = Instance.new("TextButton")
            local Frame = Instance.new("Frame")
            local tick = Instance.new("ImageLabel")
            local ImageLabel = Instance.new("ImageLabel")
            local TextLabel = Instance.new("TextLabel")

            Toggle.Name = "Toggle"
            Toggle.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder.ScrollingFrame
            Toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Toggle.BorderSizePixel = 0
            Toggle.Size = UDim2.new(0, 350, 0, 25)
            Toggle.Font = Enum.Font.SourceSans
            Toggle.Text = ""
            Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
            Toggle.TextSize = 14.000

            Frame.Parent = Toggle
            Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Frame.BorderColor3 = Color3.fromRGB(144, 89, 34)
            Frame.BorderSizePixel = 3
            Frame.Position = UDim2.new(0.932142854, 0, 0.0700000003, 0)
            Frame.Size = UDim2.new(0, 20, 0, 20)

            tick.Name = "tick"
            tick.Parent = Toggle
            tick.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            tick.BackgroundTransparency = 1.000
            tick.Position = UDim2.new(0.934285641, 0, 0.119999997, 0)
            tick.Size = UDim2.new(0, 19, 0, 19)
            tick.Visible = value
            tick.Image = "rbxassetid://10482937399"

            ImageLabel.Parent = Toggle
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.Size = UDim2.new(0, 25, 0, 25)
            ImageLabel.ZIndex = -1
            ImageLabel.Image = "rbxassetid://7832083744"

            TextLabel.Parent = Toggle
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.BackgroundTransparency = 1.000
            TextLabel.Position = UDim2.new(0.0914285779, 0, 0, 0)
            TextLabel.Size = UDim2.new(0, 200, 0, 25)
            TextLabel.Font = Enum.Font.Gotham
            TextLabel.Text = name
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.TextSize = 14.000
            TextLabel.TextXAlignment = Enum.TextXAlignment.Left

            local toggled = value or false
            Toggle.MouseButton1Click:Connect(function()
                if toggled then
                    toggled = false
                    tick.Visible = false
                else
                    toggled = true
                    tick.Visible = true
                end
                pcall(function()
                    call(toggled)
                end)
            end)
        end

        function content:Label(text)
            local Label = Instance.new("TextLabel")

            Label.Name = "Label"
            Label.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder.ScrollingFrame
            Label.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Label.BackgroundTransparency = 1.000
            Label.Size = UDim2.new(0, 350, 0, 25)
            Label.Font = Enum.Font.Gotham
            Label.Text = text
            Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Label.TextSize = 14.000
        end

        function content:TextBox(name,holder,call)
            local TextBox = Instance.new("Frame")
            local TextLabel = Instance.new("TextLabel")
            local TextBox_2 = Instance.new("TextBox")

            TextBox.Name = "TextBox"
            TextBox.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder.ScrollingFrame
            TextBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            TextBox.BorderSizePixel = 0
            TextBox.Size = UDim2.new(0, 350, 0, 25)

            TextLabel.Parent = TextBox
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.BackgroundTransparency = 1.000
            TextLabel.Size = UDim2.new(0, 213, 0, 25)
            TextLabel.Font = Enum.Font.Gotham
            TextLabel.Text = name
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.TextSize = 14.000

            TextBox_2.Parent = TextBox
            TextBox_2.BackgroundColor3 = Color3.fromRGB(170, 170, 170)
            TextBox_2.Position = UDim2.new(0.60857141, 0, 0, 0)
            TextBox_2.Size = UDim2.new(0, 137, 0, 25)
            TextBox_2.Font = Enum.Font.Gotham
            TextBox_2.Text = holder
            TextBox_2.TextColor3 = Color3.fromRGB(0, 0, 0)
            TextBox_2.TextSize = 14.000

            TextBox_2.FocusLost:Connect(function()
                if #TextBox_2.Text > 0 then
                    call(TextBox_2.Text)
                end
            end)
        end

        function content:Line()
            local Line = Instance.new("Frame")

            Line.Name = "Line"
            Line.Parent = game.StarterGui.VietHUb.MainFrame.ContainerHolder.ScrollingFrame
            Line.BackgroundColor3 = Color3.fromRGB(144, 89, 34)
            Line.BorderSizePixel = 0
            Line.Position = UDim2.new(0.018236991, 0, 0.441471577, 0)
            Line.Size = UDim2.new(0, 376, 0, 2)
        end
        return content

    end
    return win
end
return viethub
